page{
  background-color: whitesmoke
}
.nav_swiper {
  width: 100%;
  height: 300rpx;
}

.slide-image {
  width: 100%;
  height: 300rpx;
}

.center {
  height: 230rpx;
  width: 100%;
  background-color: rgb(245, 245, 245);
}

.center_left {
  float: left;
  margin: 25rpx;
  width: 43%;
  height: 180rpx;
  border-right: 3rpx solid rgb(206, 206, 206);
  border-radius: 10rpx;
  background-color: white;
}

.center_right {
  float: right;
  margin: 25rpx;
  width: 43%;
  height: 180rpx;
  border-left: 3rpx solid rgb(206, 206, 206);
  border-radius: 10rpx;
  background-color: white;
}

.center_image {
  position: relative;
  top: 43rpx;
  left: 25rpx;
  width: 100rpx;
  height: 100rpx;
}

.center_text {
  position: relative;
  top: 5rpx;
  left: 35rpx;
}

.bottom {
  position: relative;
  width: 100%;
  height: 364rpx;
  background-color: white;
}

.bottom_part {
  float: left;
  width: 25%;
  height: 180rpx;
  margin: 0rpx;
  border-bottom: 1rpx solid gainsboro;
}

.bottom_icon {
  position: relative;
  top: 50rpx;
  left: 60rpx;
  width: 55rpx;
  height: 50rpx;
  margin: 0;
}

.bottom_text {
  display: block;
  position: relative;
  top: 55rpx;
  left: -5rpx;
  height: 30rpx;
  width: 180rpx;
  font-size: 28rpx;
  text-align: center;
  margin: 0;
}

.mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  z-index: 9000;
  opacity: 0.7;
}

.modalDlg {
  width: 290rpx;
  height: 310rpx;
  position: fixed;
  top: 60%;
  left: 0;
  z-index: 9999;
  margin: -370rpx 230rpx;
  background-color: whitesmoke;
  color: #1296db;
  border-radius: 36rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.modalDlg_text {
  align-items: center;
  font-size: 30rpx;
  padding: 10rpx 0 10rpx 0;
}

.modalDlg_image {
  align-items: center;
  padding: 40rpx 0 0 0;
  height: 150rpx;
  width: 150rpx;
}

.modalDlg_close {
  align-items: center;
  width: 50rpx;
  height: 50rpx;
}
